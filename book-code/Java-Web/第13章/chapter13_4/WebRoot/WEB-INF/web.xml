<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>AddInfoServlet</servlet-name>
    <servlet-class>com.itzcn.servlet.AddInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpInfoServlet</servlet-name>
    <servlet-class>com.itzcn.servlet.UpInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DelInfoServlet</servlet-name>
    <servlet-class>com.itzcn.servlet.DelInfoServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AddInfoServlet</servlet-name>
    <url-pattern>/servlet/AddInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpInfoServlet</servlet-name>
    <url-pattern>/servlet/UpInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DelInfoServlet</servlet-name>
    <url-pattern>/servlet/DelInfoServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
